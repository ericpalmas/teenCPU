<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="homeRoot" alignment="CENTER" prefHeight="650.0" prefWidth="1100.0" spacing="2.0" styleClass="main-background" stylesheets="@../style/home-view-style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.supsi.teencpu.javafx.jfxcontrollers.HomeController">
   <children>
      <ImageView fx:id="logoImageView" fitHeight="326.0" fitWidth="283.0" opacity="0.84" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logo2.png" />
         </image>
      </ImageView>
      <Pane prefHeight="13.0" prefWidth="860.0" />
      <Button fx:id="startNewSimBtn" alignment="CENTER" blendMode="SRC_ATOP" contentDisplay="CENTER" mnemonicParsing="false" onAction="#startNewSimulation" styleClass="home-buttons" stylesheets="@../style/home-view-style.css" text="%new.simulation.button" textAlignment="CENTER">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
